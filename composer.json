{
  "name": "efureev/laravel-trees",
  "description": "Multi-Tree structures for Laravel",
  "license": "MIT",
  "keywords": [
    "php",
    "tree",
    "multi-tree",
    "laravel",
    "package"
  ],
  "authors": [
    {
      "name": "Eugene Fureev",
      "email": "fureev@gmail.com"
    }
  ],
  "require": {
    "php": "^8.4",
    "illuminate/database": "^11.36",
    "illuminate/events": "^11.36",
    "efureev/support": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5",
    "orchestra/testbench": "^9.8.0",
    "fakerphp/faker": "^1.24",
    "symfony/var-dumper": "^7.2",
    "squizlabs/php_codesniffer": "^3.11"
  },
  "autoload": {
    "psr-4": {
      "Fureev\\Trees\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fureev\\Trees\\Tests\\": "tests/",
      "Fureev\\Trees\\Tests\\Database\\Factories\\": "tests/database/factories"
    }
  },
  "scripts": {
    "phpcs": "@php ./vendor/bin/phpcs",
    "cs-fix": "@php ./vendor/bin/phpcbf",
    "test": "@php ./vendor/bin/phpunit --no-coverage --testdox",
    "test-cover": "@php ./vendor/bin/phpunit --coverage-text",
    "phpstan": "@php ./vendor/bin/phpstan analyze --ansi"
  }
}
